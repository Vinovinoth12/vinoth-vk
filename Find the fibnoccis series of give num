#include<iostream>
#include<conio.h>

int main()
{
	int arr[100],i,n;
	cout<<"Enter Number of Fibonacci Terms required:";
	cin>>n;
	arr[0]=0;
	arr[1]=1; 
	for(i=2;i<n;i++)
	  arr[i]=arr[i-1]+arr[i-2];
	
	cout<<"Required "<<n<<" Fibonaccii Terms are:"<<endl;
	for (i=0;i<n;i++)
	cout<<arr[i]<<", ";

	return 0;
}
